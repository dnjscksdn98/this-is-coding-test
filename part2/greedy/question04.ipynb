   "source": [
    "### 나의 소스코드"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "25 3\n",
      "6\n"
     ]
    }
   ],
   "source": [
    "n, k = map(int, input().split())\n",
    "\n",
    "cnt = 0\n",
    "while n > 1:\n",
    "    if n % k == 0:\n",
    "        n /= k\n",
    "    \n",
    "    else:\n",
    "        n -= 1\n",
    "    cnt += 1\n",
    "    \n",
    "print(cnt)"
   ]
  }
